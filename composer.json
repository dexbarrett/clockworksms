{
    "name": "dexbarrett/clockworksms",
    "description": "A PHP wrapper for using Clockwork SMS API",
    "keywords": [
        "clockwork", "api", "wrapper", "sms"
    ],
    "homepage": "https://github.com/dexbarrett/clockworksms",
    "license": "MIT",
    "authors": [
        {
            "name": "Judas Borbon",
            "email": "judas.borbon@gmail.com",
            "homepage": "https://jborbon.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "2.*",
        "mockery/mockery": "^0.9.6"
    },
    "autoload": {
        "psr-4": {
            "DexBarrett\\ClockworkSms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DexBarrett\\ClockworkSms\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
